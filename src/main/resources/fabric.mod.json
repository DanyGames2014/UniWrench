{
  "schemaVersion": 1,
  "id": "uniwrench",
  "version": "${version}",

  "name": "UniWrench",
  "description": "This adds an universal wrench",
  "authors": [
    "DanyGames2014"
  ],

  "license": "MIT",
  "icon": "assets/uniwrench/icon.png",

  "environment": "*",
  "entrypoints": {
